__fastcall TGroupModal::TGroupModal(TComponent* Owner)
	: TModalEntityForm(Owner)
{
	emptyEntity=new Group("",true,0,0);
	entity=new Group(*emptyEntity);
	tempEntity==new Group(*emptyEntity);
}

